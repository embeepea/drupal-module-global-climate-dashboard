    <timeslider />
    <mugloverrides>
      <element name="horizontalaxis">
        <attribute name="min" value="1950" />
        <attribute name="max" value="2015" />
        <element name="binding">
          <attribute name="id" value="Tab1TimeBinding" />
        </element>
        <element name="pan">
          <attribute name="min" value="1880" />
