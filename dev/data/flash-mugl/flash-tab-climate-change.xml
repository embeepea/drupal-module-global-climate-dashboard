        <element name="zoom">
          <attribute name="min" value="6M" />
        </element>
      </element>
    </mugloverrides>

    <graphRef id="flash-graph-temperature" />
    <graphRef id="flash-graph-co2" />
    <graphRef id="flash-graph-snowcover" />
    <graphRef id="flash-graph-sealevel" />
    <graphRef id="flash-graph-asi" />
    <graphRef id="flash-graph-oceanheat" />
    <graphRef id="flash-graph-sunsenergy" />
